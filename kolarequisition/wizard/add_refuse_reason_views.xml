<?xml version="1.0" encoding="utf-8"?>
<odoo>
	<record id="view_add_refuse_reason_form" model="ir.ui.view">
		<field name="name">kolarequest.refuse.reason.form</field>
		<field name="model">kolarequest.refuse.reason</field>
		<field name="arch" type="xml">
			<form string="RefuseReason">
				<field name="request_refuse_reason"/>
				<footer>
					<button string="Refuse" name="refuse_reason" type="object" class='btn-primary'/>
					<button string="Cancel" class='btn-default' special="cancel"/>
				</footer>
			</form>
		</field>
	</record>
</odoo>
